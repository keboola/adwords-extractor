{% extends 'KeboolaAdWordsExtractor::base.html.twig' %}

{% block content %}
    {{ form_start(form, {'attr': {'class': 'form_horizontal'}}) }}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_label(form.token) }}
        {{ form_errors(form.token) }}
        {{ form_widget(form.token) }}
    </div>
    <div class="form-group">
            {{ form_widget(form.send) }}
    </div>
    {{ form_end(form) }}
{% endblock %}