{
    "jobs_ex-adwords": {
        "_all" : {
            "enabled" : true,
            "analyzer": "snowball"
        },
        "date_detection": false,
        "numeric_detection": false,
        "dynamic": "strict",
        "properties": {
            "id": {
                "type": "long"
            },
            "runId": {
                "index": "not_analyzed",
                "type": "string"
            },
            "lockName": {
                "index": "not_analyzed",
                "type": "string"
            },
            "project": {
                "dynamic": "strict",
                "properties": {
                    "id": {
                        "type": "long"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            },
            "token": {
                "dynamic": "strict",
                "properties": {
                    "id": {
                        "type": "long"
                    },
                    "description": {
                        "type": "string"
                    },
                    "token": {
                        "index": "not_analyzed",
                        "type": "string"
                    }
                }
            },
            "component": {
                "index": "not_analyzed",
                "type": "string"
            },
            "command": {
                "index": "not_analyzed",
                "type": "string"
            },
            "params": {
                "dynamic": "strict",
                "properties": {
                    "config": {
                        "type": "string"
                    }
                }
            },
            "result": {
                "type": "object",
                "enabled": false
            },
            "status": {
                "type": "string"
            },
            "process": {
                "dynamic": "strict",
                "properties": {
                    "host": {
                        "type": "string"
                    },
                    "pid": {
                        "type": "long"
                    }
                }
            },
            "createdTime": {
                "format": "dateOptionalTime",
                "type": "date"
            },
            "startTime": {
                "format": "dateOptionalTime",
                "type": "date"
            },
            "endTime": {
                "format": "dateOptionalTime",
                "type": "date"
            },
            "durationSeconds": {
                "type": "long"
            },
            "waitSeconds": {
                "type": "long"
            }
        }
    }
}
