{
    "name": "keboola/adwords-extractor",
    "type": "symfony-bundle",
    "description": "Keboola AdWords extractor",
    "keywords": ["keboola", "adwords", "extractor"],
    "authors": [
        {
            "name": "Jakub Matejka",
            "email": "jakub@keboola.com"
        }
    ],
    "require": {
        "ext-soap": "*",
        "keboola/syrup": "~2.0",
        "googleads/googleads-php-lib": "~5.5.1"
    },
    "require-dev": {
        "symfony/var-dumper": "~2.6",
        "phpunit/phpunit": "~4.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-update-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Keboola\\AdWordsExtractor": ""
        }
    },
    "target-dir": "Keboola/AdWordsExtractor",
    "extra": {
        "syrup-app-name": "ex-adwords",
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web"
    }
}
